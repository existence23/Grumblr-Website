[Unit]
Description=gunicorn daemon
After=network.target
[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/kge/homework/6/webapps
ExecStart=/home/ubuntu/kge/homework/6/venv/bin/gunicorn --workers 3 --bind unix:/home/ubuntu/kge/homework/6/webapps/webapps.sock webapps.wsgi:application
[Install]
WantedBy=multi-user.target
